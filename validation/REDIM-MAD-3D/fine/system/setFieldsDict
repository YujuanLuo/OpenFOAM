FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue theta1 1
    volScalarFieldValue theta2 97
);

regions
(
    boxToCell
    {
        box (0.02 0 0) (0.03 0.004 0.004);

        fieldValues
        (
   		volScalarFieldValue theta1 101
        );
    }
);

